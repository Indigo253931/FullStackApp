<h1>All Strains</h1>
<a class="btn" href="/strains/new">Add a Strain</a>
<div class="strains">
  <% @strains.each do |strain| %>
  <div class="row">
        <div class ="strain_list">
         <h2 class = "strain_name">
          <a href="/strains/<%= strain.id %>">
            <%= strain.name %>
          </a>
        </h2>
          <ul>
          <li>
            <%= strain.rating %>
            </li>
    </ul>
    </div>
  </div>
  <% end %>
</div>
