<nav>
<a class ="all_strains_page" href="/strains"> All Strains</a>
</nav>
<div class ="strain_list">
<h1 id = "new_title">New Strain</h1>
</div>
<div class ="strain_list">
<form action="/strains" method="post">
  <label for="strain[name]">Name:</label>
  <input class = "edit_form_input" name="strain[name]" placeholder="Mango Kush">
  <label for="strain[rating]">Rating:</label>
  <input class = "edit_form_input" name="strain[rating]" placeholder="4.5">
   <label for="strain[kind]">Kind:</label>
  <input class = "edit_form_input" name="strain[kind]" placeholder="Hybrid">
  <label for="strain[review]">Review:</label>
  <input class = "edit_form_input" name="strain[review]" placeholder="Top quality">
  <label for="strain[media]">URL:</label>
  <input class = "edit_form_input" name="strain[media]" placeholder="URL here" value='<%= "#{@strain.media}" %>'>
  <input id = "save_btn" class = "edit_form_input" type="submit" value="Create">
</form>
</div>